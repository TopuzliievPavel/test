ul.main-features_list
  li(
    v-for="item, index in content.featuresList"
    )
    router-link.main-features_link(:to="item.link")
      .main-features_icon-holder
        i(aria-hidden="true"
          v-bind:class="item.iconClass"
        )
      h5.h5 {{item.featureTitle}}
      p {{item.featureDescription}}
